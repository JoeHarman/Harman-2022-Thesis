	pmid = {18772397},
	pmcid = {PMC2848990},
	keywords = {Adenocarcinoma, Algorithms, Carcinoma, Pancreatic Ductal, Computational Biology, Gene Amplification, Gene Expression Profiling, Genome, Human, Humans, Models, Molecular, Mutation, Mutation, Missense, Oligonucleotide Array Sequence Analysis, Pancreatic Neoplasms, Point Mutation, Polymorphism, Single Nucleotide, Sequence Deletion, Signal Transduction},
