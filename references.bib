	pmid = {23303821},
	keywords = {Animals, B-Lymphocytes, Binding Sites, Cell Cycle, Cell Differentiation, Cell Lineage, Flow Cytometry, Fluorescent Antibody Technique, Gene Expression Regulation, Genome, Ikaros Transcription Factor, Lymphocyte Activation, Mice, Precursor Cells, B-Lymphoid, Signal Transduction, Transcription Factors},
