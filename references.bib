	keywords = {Animals, Animals, Genetically Modified, Aorta, Cell Differentiation, Cell Lineage, Cell Separation, Endothelial Cells, Endothelium, Vascular, Female, Flow Cytometry, Genes, Reporter, Hematopoietic Stem Cells, Male, Microscopy, Confocal, Microscopy, Fluorescence, Transgenes, Zebrafish},
