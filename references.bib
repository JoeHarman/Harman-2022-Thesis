	issn = {1476-5594},
	url = {https://www.nature.com/articles/1207381},
	doi = {10.1038/sj.onc.1207381},
	abstract = {Treating lung cancer cell lines using low-dose 5-aza-2′-deoxycytidine (DAC) caused an accumulation of procaspase-9 through mRNA upregulation, but the cells did not undergo apoptosis. However, when cells were treated with DAC and infected with a low dose of a recombinant wild-type p53 adenovirus vector (Ad-p53), a synergistic growth inhibitory effect was observed. Combination treatment induced Apaf-1 and procaspase-9 expression in which cytochrome c releases by Ad-p53 triggered the mitochondrial pathway of apoptosis. Selective blockage of caspase-9 activities by Z-LEHD-FMK completely attenuated DAC-induced enhancement of apoptosis mediated by Ad-p53 infection, and ectopic overexpression of procaspase-9 sensitized cells to Ad-p53-induced apoptosis in p53-null cells. In addition, DAC sensitized lung cancer cells to cisplatin and paclitaxel. Induction of the mitochondrial pathway of apoptosis using a slightly toxic dose of DAC may therefore be a strategy for treating lung cancer, and DAC treatment may have clinical implications when combined with chemotherapy or apoptosis-inducing gene therapy.},
	language = {en},
	number = {40},
	urldate = {2021-04-03},
	journal = {Oncogene},
	author = {Gomyo, Yoshihito and Sasaki, Ji-ichiro and Branch, Cynthia and Roth, Jack A. and Mukhopadhyay, Tapas},
	month = sep,
	year = {2004},
	pages = {6779--6787},
}

