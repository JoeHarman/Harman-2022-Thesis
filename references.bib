	number = {2},
	journal = {Cell Reports},
	author = {Kerry, Jon and Godfrey, Laura and Repapi, Emmanouela and Tapia, Marta and Blackledge, Neil P. and Ma, Helen and Ballabio, Erica and O'Byrne, Sorcha and Ponthan, Frida and Heidenreich, Olaf and Roy, Anindita and Roberts, Irene and Konopleva, Marina and Klose, Robert J. and Geng, Huimin and Milne, Thomas A.},
