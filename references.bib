	pmcid = {PMC5865415},
	pages = {565--574},
}

@article{wang_activation_2013,
	title = {Activation of {Rac1} {GTPase} promotes leukemia cell chemotherapy resistance, quiescence and niche interaction},
	volume = {7},
	issn = {1574-7891},
	url = {https://www.ncbi.nlm.nih.gov/pmc/articles/PMC5528460/},
	doi = {10.1016/j.molonc.2013.05.001},
	abstract = {Leukemia stem cells (LSCs) reside in bone marrow niche and receive important signals from the microenvironment that support self‐renewal, maintain quiescence and endow LSC with the ability of chemotherapy resistance. Rac1 belongs to the small GTP‐binding protein superfamily and is implicated in the interactions of hematopoietic progenitors and bone marrow niche. Our previous studies have shown that Rac1 is over‐expressed in leukemia patients and activation of Rac1 GTPase is closely associated with the efficient migration of leukemia cells. However, the potential functions for Rac1 GTPase in LSCs behaviors and in the residence of leukemia cells in niche remain unknown. In this study, by forced expression of a dominant‐negative form of Rac1 GTPase in a CD34+ myeloid leukemia cell line, as well as bone marrow cells from leukemia patients, we show that inactivation of Rac1 GTPase causes impaired migration and enhances chemotherapeutic sensitivity. Inactivation of Rac1 in leukemia cells also lead to a reduction in the frequency of cells in quiescent state and inhibition of homing to bone marrow niche. Gene expression analysis shows that inactivation of Rac1 down‐regulates the expression of several cell intrinsic cell cycle inhibitors such as p21, p27, and p57, as well as the extrinsic molecules that mediated the interaction of LSC with osteoblastic niche. Furthermore, we show that Rac1 mediated the localization in niche is further attributed to the maintenance of quiescence. Our results provide evidence for the critical role of Rac1 GTPase in leukemia cell chemotherapy resistance, quiescence maintenance and the interaction with bone marrow microenvironment.},
	number = {5},
	urldate = {2019-09-20},
	journal = {Molecular Oncology},
	author = {Wang, Ji-Ying and Yu, Pei and Chen, Shuying and Xing, Haiyan and Chen, Yirui and Wang, Min and Tang, Kejing and Tian, Zheng and Rao, Qing and Wang, Jianxiang},
	month = oct,
	year = {2013},
	pmid = {23726395},
	pmcid = {PMC5528460},
	pages = {907--916},
}

@article{durand-onayli_rac_2018,
	title = {Rac {GTPases} in {Hematological} {Malignancies}},
	volume = {19},
	issn = {1422-0067},
	url = {https://www.ncbi.nlm.nih.gov/pmc/articles/PMC6321480/},
	doi = {10.3390/ijms19124041},
	abstract = {Emerging evidence suggests that crosstalk between hematologic tumor cells and the tumor microenvironment contributes to leukemia and lymphoma cell migration, survival, and proliferation. The supportive tumor cell-microenvironment interactions and the resulting cellular processes require adaptations and modulations of the cytoskeleton. The Rac subfamily of the Rho family GTPases includes key regulators of the cytoskeleton, with essential functions in both normal and transformed leukocytes. Rac proteins function downstream of receptor tyrosine kinases, chemokine receptors, and integrins, orchestrating a multitude of signals arising from the microenvironment. As such, it is not surprising that deregulation of Rac expression and activation plays a role in the development and progression of hematological malignancies. In this review, we will give an overview of the specific contribution of the deregulation of Rac GTPases in hematologic malignancies.},
	number = {12},
	urldate = {2019-09-20},
	journal = {International Journal of Molecular Sciences},
	author = {Durand-Onaylı, Valerie and Haslauer, Theresa and Härzschel, Andrea and Hartmann, Tanja Nicole},
	month = dec,
	year = {2018},
	pmid = {30558116},
	pmcid = {PMC6321480},
}

@article{lancrin_gfi1_2012,
	title = {{GFI1} and {GFI1B} control the loss of endothelial identity of hemogenic endothelium during hematopoietic commitment},
	volume = {120},
	copyright = {© 2012 by The American Society of Hematology},
	issn = {0006-4971, 1528-0020},
	url = {http://www.bloodjournal.org/content/120/2/314},
	doi = {10.1182/blood-2011-10-386094},
	abstract = {Recent studies have established that during embryonic development, hematopoietic progenitors and stem cells are generated from hemogenic endothelium precursors through a process termed endothelial to hematopoietic transition (EHT). The transcription factor RUNX1 is essential for this process, but its main downstream effectors remain largely unknown. Here, we report the identification of Gfi1 and Gfi1b as direct targets of RUNX1 and critical regulators of EHT. GFI1 and GFI1B are able to trigger, in the absence of RUNX1, the down-regulation of endothelial markers and the formation of round cells, a morphologic change characteristic of EHT. Conversely, blood progenitors in Gfi1- and Gfi1b-deficient embryos maintain the expression of endothelial genes. Moreover, those cells are not released from the yolk sac and disseminated into embryonic tissues. Taken together, our findings demonstrate a critical and specific role of the GFI1 transcription factors in the first steps of the process leading to the generation of hematopoietic progenitors from hemogenic endothelium.},
	language = {en},
	number = {2},
	urldate = {2019-09-20},
	journal = {Blood},
	author = {Lancrin, Christophe and Mazan, Milena and Stefanska, Monika and Patel, Rahima and Lichtinger, Monika and Costa, Guilherme and Vargel, Özge and Wilson, Nicola K. and Möröy, Tarik and Bonifer, Constanze and Göttgens, Berthold and Kouskoff, Valerie and Lacaud, Georges},
	month = jul,
	year = {2012},
	pmid = {22668850},
	pages = {314--322},
}

