}

@article{lo_cell_2000,
	title = {Cell movement is guided by the rigidity of the substrate.},
	volume = {79},
	issn = {0006-3495},
	url = {http://www.ncbi.nlm.nih.gov/pmc/articles/PMC1300921/},
	abstract = {Directional cell locomotion is critical in many physiological processes, including morphogenesis, the immune response, and wound healing. It is well known that in these processes cell movements can be guided by gradients of various chemical signals. In this study, we demonstrate that cell movement can also be guided by purely physical interactions at the cell-substrate interface. We cultured National Institutes of Health 3T3 fibroblasts on flexible polyacrylamide sheets coated with type I collagen. A transition in rigidity was introduced in the central region of the sheet by a discontinuity in the concentration of the bis-acrylamide cross-linker. Cells approaching the transition region from the soft side could easily migrate across the boundary, with a concurrent increase in spreading area and traction forces. In contrast, cells migrating from the stiff side turned around or retracted as they reached the boundary. We call this apparent preference for a stiff substrate "durotaxis." In addition to substrate rigidity, we discovered that cell movement could also be guided by manipulating the flexible substrate to produce mechanical strains in the front or rear of a polarized cell. We conclude that changes in tissue rigidity and strain could play an important controlling role in a number of normal and pathological processes involving cell locomotion.},
	number = {1},
	urldate = {2014-11-01},
	journal = {Biophysical Journal},
	author = {Lo, C M and Wang, H B and Dembo, M and Wang, Y L},
	month = jul,
	year = {2000},
	pmid = {10866943},
	pmcid = {PMC1300921},
	pages = {144--152},
}

@article{erler_hypoxia-induced_2009,
	title = {Hypoxia-induced lysyl oxidase is a critical mediator of bone marrow cell recruitment to form the pre-metastatic niche},
	volume = {15},
	issn = {1535-6108},
	url = {http://www.ncbi.nlm.nih.gov/pmc/articles/PMC3050620/},
	doi = {10.1016/j.ccr.2008.11.012},
	abstract = {Summary
Tumor cell metastasis is facilitated by “pre-metastatic niches” formed in destination organs by invading bone marrow-derived cells (BMDCs). Lysyl oxidase (LOX) is critical for pre-metastatic niche formation. LOX secreted by hypoxic breast tumor cells accumulates at pre-metastatic sites, cross-links collagen-IV in the basement membrane, and is essential for CD11b+ myeloid cell recruitment. CD11b+ cells adhere to cross-linked collagen-IV and produce matrix metalloproteinase-2 which cleaves collagen, enhancing the invasion and recruitment of BMDCs and metastasizing tumor cells. LOX inhibition prevents CD11b+ cell recruitment and metastatic growth. CD11b+ cells and LOX also co-localize in biopsies of human metastases. Our findings demonstrate a critical role for LOX in pre-metastatic niche formation and support targeting LOX for the treatment and prevention of metastatic disease.},
	number = {1},
	urldate = {2014-11-01},
	journal = {Cancer cell},
	author = {Erler, Janine T. and Bennewith, Kevin L. and Cox, Thomas R. and Lang, Georgina and Bird, Demelza and Koong, Albert and Le, Quynh-Thu and Giaccia, Amato J.},
	month = jan,
	year = {2009},
	pmid = {19111879},
	pmcid = {PMC3050620},
	pages = {35--44},
}

@article{even-ram_cell_2005,
	series = {Cell-to-cell contact and extracellular matrix},
	title = {Cell migration in {3D} matrix},
