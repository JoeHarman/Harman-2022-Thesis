	keywords = {Animals, Cell Count, Cell Polarity, Cell Proliferation, Homeostasis, Mice, Muscle, Skeletal, Nerve Tissue Proteins, Receptors, G-Protein-Coupled, Regeneration, Satellite Cells, Skeletal Muscle, Wnt Proteins},
	pages = {535--547},
}

