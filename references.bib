@article{van_oevelen_cebp_2015,
	title = {C/{EBPα} {Activates} {Pre}-existing and {De} {Novo} {Macrophage} {Enhancers} during {Induced} {Pre}-{B} {Cell} {Transdifferentiation} and {Myelopoiesis}},
