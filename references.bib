@article{wyckoff_paracrine_2004,
	title = {A {Paracrine} {Loop} between {Tumor} {Cells} and {Macrophages} {Is} {Required} for {Tumor} {Cell} {Migration} in {Mammary} {Tumors}},
	volume = {64},
	issn = {0008-5472, 1538-7445},
	url = {http://cancerres.aacrjournals.org/content/64/19/7022},
	doi = {10.1158/0008-5472.CAN-04-1449},
	abstract = {Invasion of tumor cells into the surrounding connective tissue and blood vessels is a key step in the metastatic spread of breast tumors. Although the presence of macrophages in primary tumors is associated with increased metastatic potential, the mechanistic basis for this observation is unknown. Using a chemotaxis-based in vivo invasion assay and multiphoton-based intravital imaging, we show that the interaction between macrophages and tumor cells facilitates the migration of carcinoma cells in the primary tumor. Gradients of either epidermal growth factor (EGF) or colony-stimulating factor 1 (CSF-1) stimulate collection into microneedles of tumor cells and macrophages even though tumor cells express only EGF receptor and macrophages express only CSF-1 receptor. Intravital imaging shows that macrophages and tumor cells migrate toward microneedles containing either EGF or CSF-1. Inhibition of either CSF-1– or EGF-stimulated signaling reduces the migration of both cell types. This work provides the first direct evidence for a synergistic interaction between macrophages and tumor cells during cell migration in vivo and indicates a mechanism for how macrophages may contribute to metastasis.},
	language = {en},
	number = {19},
	urldate = {2014-11-02},
	journal = {Cancer Research},
	author = {Wyckoff, Jeffrey and Wang, Weigang and Lin, Elaine Y. and Wang, Yarong and Pixley, Fiona and Stanley, E. Richard and Graf, Thomas and Pollard, Jeffrey W. and Segall, Jeffrey and Condeelis, John},
	month = oct,
	year = {2004},
	pmid = {15466195},
	pages = {7022--7029},
}

@article{wicki_tumor_2006,
	title = {Tumor invasion in the absence of epithelial-mesenchymal transition: podoplanin-mediated remodeling of the actin cytoskeleton},
	volume = {9},
	issn = {1535-6108},
	shorttitle = {Tumor invasion in the absence of epithelial-mesenchymal transition},
	doi = {10.1016/j.ccr.2006.03.010},
	abstract = {The expression of podoplanin, a small mucin-like protein, is upregulated in the invasive front of a number of human carcinomas. We have investigated podoplanin function in cultured human breast cancer cells, in a mouse model of pancreatic beta cell carcinogenesis, and in human cancer biopsies. Our results indicate that podoplanin promotes tumor cell invasion in vitro and in vivo. Notably, the expression and subcellular localization of epithelial markers are unaltered, and mesenchymal markers are not induced in invasive podoplanin-expressing tumor cells. Rather, podoplanin induces collective cell migration by filopodia formation via the downregulation of the activities of small Rho family GTPases. In conclusion, podoplanin induces an alternative pathway of tumor cell invasion in the absence of epithelial-mesenchymal transition (EMT).},
	language = {eng},
	number = {4},
	journal = {Cancer Cell},
	author = {Wicki, Andreas and Lehembre, François and Wick, Nikolaus and Hantusch, Brigitte and Kerjaschki, Dontscho and Christofori, Gerhard},
	month = apr,
