	pmid = {19321859},
	keywords = {Animals, Core Binding Factor Alpha 2 Subunit, Embryo, Mammalian, Enhancer Elements, Genetic, Hematopoietic Stem Cells, Mice, Mice, Transgenic, Promoter Regions, Genetic, Species Specificity, Transcription, Genetic},
	pages = {5121--5124},
