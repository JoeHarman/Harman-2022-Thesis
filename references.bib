	keywords = {Animals, Cell Differentiation, Cell Proliferation, Cells, Cultured, Mice, Mice, Nude, Muscle, Skeletal, Myofibrils, Regeneration, Satellite Cells, Skeletal Muscle, Stem Cells},
	pages = {289--301},
}

