@article{wang_aml1_2010,
	title = {{AML1} is overexpressed in patients with myeloproliferative neoplasms and mediates {JAK2V617F}-independent overexpression of {NF}-{E2}},
	volume = {116},
	issn = {1528-0020},
	doi = {10.1182/blood-2009-11-254664},
	abstract = {The transcription factor NF-E2 is overexpressed in the majority of patients with polycythemia vera (PV). Concomitantly, 95\% of these patients carry the JAK2(V617F) mutation. Although NF-E2 levels correlate with JAK2(V671F) allele burden in some PV cohorts, the molecular mechanism causing aberrant NF-E2 expression has not been described. Here we show that NF-E2 expression is also increased in patients with essential thrombocythemia and primary myelofibrosis independent of the presence of the JAK2(V617F) mutation. Characterization of the NF-E2 promoter revealed multiple functional binding sites for AML1/RUNX-1. Chromatin immunoprecipitation demonstrated AML1 binding to the NF-E2 promoter in vivo. Moreover, AML1 binding to the NF-E2 promoter was significantly increased in granulocytes from PV patients compared with healthy controls. AML1 mRNA expression was elevated in patients with PV, essential thrombocythemia, and primary myelofibrosis both in the presence and absence of JAK2(V617F). In addition, AML1 and NF-E2 expression were highly correlated. RNAi-mediated suppression of either AML1 or of its binding partner CBF-beta significantly decreased NF-E2 expression. Moreover, expression of the leukemic fusion protein AML/ETO drastically decreased NF-E2 protein levels. Our data identify NF-E2 as a novel AML1 target gene and delineate a role for aberrant AML1 expression in mediating elevated NF-E2 expression in MPN patients.},
	language = {eng},
	number = {2},
	journal = {Blood},
	author = {Wang, Wei and Schwemmers, Sven and Hexner, Elizabeth O. and Pahl, Heike L.},
	month = jul,
	year = {2010},
	pmid = {20339092},
	pmcid = {PMC2910609},
	keywords = {Blotting, Northern, Blotting, Western, Chromatin Immunoprecipitation, Core Binding Factor Alpha 2 Subunit, Electrophoretic Mobility Shift Assay, Gene Expression, Gene Expression Regulation, Neoplastic, Humans, Janus Kinase 2, Myeloproliferative Disorders, NF-E2 Transcription Factor, p45 Subunit, Promoter Regions, Genetic, RNA Interference, Reverse Transcriptase Polymerase Chain Reaction, Transfection},
	pages = {254--266},
}

@article{hoogenkamp_early_2009,
	title = {Early chromatin unfolding by {RUNX1}: a molecular explanation for differential requirements during specification versus maintenance of the hematopoietic gene expression program},
	volume = {114},
	issn = {0006-4971},
	shorttitle = {Early chromatin unfolding by {RUNX1}},
	url = {https://www.ncbi.nlm.nih.gov/pmc/articles/PMC2714206/},
	doi = {10.1182/blood-2008-11-191890},
	abstract = {At the cellular level, development progresses through successive regulatory states, each characterized by their specific gene expression profile. However, the molecular mechanisms regulating first the priming and then maintenance of gene expression within one developmental pathway are essentially unknown. The hematopoietic system represents a powerful experimental model to address these questions and here we have focused on a regulatory circuit playing a central role in myelopoiesis: the transcription factor PU.1, its target gene colony-stimulating-factor 1 receptor (Csf1r), and key upstream regulators such as RUNX1. We find that during ontogeny, chromatin unfolding precedes the establishment of active histone marks and the formation of stable transcription factor complexes at the Pu.1 locus and we show that chromatin remodeling is mediated by the transient binding of RUNX1 to Pu.1 cis-elements. By contrast, chromatin reorganization of Csf1r requires prior expression of PU.1 together with RUNX1 binding. Once the full hematopoietic program is established, stable transcription factor complexes and active chromatin can be maintained without RUNX1. Our experiments therefore demonstrate how individual transcription factors function in a differentiation stage–specific manner to differentially affect the initiation versus maintenance of a developmental program.},
	number = {2},
	urldate = {2022-10-03},
	journal = {Blood},
	author = {Hoogenkamp, Maarten and Lichtinger, Monika and Krysinska, Hanna and Lancrin, Christophe and Clarke, Deborah and Williamson, Andrew and Mazzarella, Luca and Ingram, Richard and Jorgensen, Helle and Fisher, Amanda and Tenen, Daniel G. and Kouskoff, Valerie and Lacaud, Georges and Bonifer, Constanze},
	month = jul,
	year = {2009},
	pmid = {19339695},
	pmcid = {PMC2714206},
	pages = {299--309},
}

@article{goumans_balancing_2002,
	title = {Balancing the activation state of the endothelium via two distinct {TGF}-beta type {I} receptors},
	volume = {21},
	issn = {0261-4189},
	doi = {10.1093/emboj/21.7.1743},
	abstract = {The generation of mice lacking specific components of the transforming growth factor-beta (TGF-beta) signal tranduction pathway shows that TGF-beta is a key player in the development and physiology of the cardiovascular system. Both pro- and anti-angiogenic properties have been ascribed to TGF-beta, for which the molecular mechanisms are unclear. Here we report that TGF-beta can activate two distinct type I receptor/Smad signalling pathways with opposite effects. TGF-beta induces phosphorylation of Smad1/5 and Smad2 in endothelial cells and these effects can be blocked upon selective inhibition of ALK1 or ALK5 expression, respectively. Whereas the TGF-beta/ALK5 pathway leads to inhibition of cell migration and proliferation, the TGF-beta/ALK1 pathway induces endothelial cell migration and proliferation. We identified genes that are induced specifically by TGF-beta-mediated ALK1 or ALK5 activation. Id1 was found to mediate the TGF-beta/ALK1-induced (and Smad-dependent) migration, while induction of plasminogen activator inhibitor-1 by activated ALK5 may contribute to the TGF-beta-induced maturation of blood vessels. Our results suggest that TGF-beta regulates the activation state of the endothelium via a fine balance between ALK5 and ALK1 signalling.},
	language = {eng},
	number = {7},
	journal = {The EMBO journal},
	author = {Goumans, Marie-José and Valdimarsdottir, Gudrun and Itoh, Susumu and Rosendahl, Alexander and Sideras, Paschalis and ten Dijke, Peter},
	month = apr,
	year = {2002},
	pmid = {11927558},
	pmcid = {PMC125949},
	keywords = {Activin Receptors, Type I, Animals, COS Cells, Cattle, Cell Division, Cells, Cultured, Chlorocebus aethiops, DNA-Binding Proteins, Dose-Response Relationship, Drug, Endothelium, Vascular, Helix-Loop-Helix Motifs, Inhibitor of Differentiation Protein 1, Kinetics, Phosphoproteins, Phosphorylation, Plasminogen Activator Inhibitor 1, Protein Serine-Threonine Kinases, Receptor, Transforming Growth Factor-beta Type I, Receptors, Transforming Growth Factor beta, Repressor Proteins, Signal Transduction, Smad2 Protein, Smad5 Protein, Trans-Activators, Transcription Factors, Transforming Growth Factor beta, Transforming Growth Factor beta1, Transforming Growth Factor beta3},
