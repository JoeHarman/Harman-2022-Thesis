	pmid = {24439806},
	keywords = {Age Factors, Cell Lineage, Cell Transplantation, Humans, Models, Biological, Muscle Development, Muscle, Skeletal, PAX7 Transcription Factor, Regeneration, Regenerative Medicine, Satellite Cells, Skeletal Muscle},
