	keywords = {Algorithms, Analysis of Variance, Base Composition, Biostatistics, Databases, Nucleic Acid, Gene Expression Profiling, High-Throughput Nucleotide Sequencing, Humans, Oligonucleotide Array Sequence Analysis, Sequence Analysis, RNA},
	pages = {204--216},
}

