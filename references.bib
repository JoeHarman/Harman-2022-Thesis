@article{rougerie_rho_2012,
	title = {Rho {GTPases}: {Masters} of {T} lymphocyte migration and activation},
	volume = {142},
	issn = {0165-2478},
	shorttitle = {Rho {GTPases}},
	url = {http://www.sciencedirect.com/science/article/pii/S0165247811002720},
