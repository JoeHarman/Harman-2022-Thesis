	pmcid = {PMC6446481},
	keywords = {Data science, Gene regulatory network, Genomics, Network inference, Precision medicine, Prostate cancer, Systems biology},
	pages = {38--48},
}

@article{joanito_incoherent_2018,
	title = {An incoherent feed-forward loop switches the {Arabidopsis} clock rapidly between two hysteretic states},
	volume = {8},
	issn = {2045-2322},
	doi = {10.1038/s41598-018-32030-z},
	abstract = {In higher plants (e.g., Arabidopsis thaliana), the core structure of the circadian clock is mostly governed by a repression process with very few direct activators. With a series of simplified models, we studied the underlying mechanism and found that the Arabidopsis clock consists of type-2 incoherent feed-forward loops (IFFLs), one of them creating a pulse-like expression in PRR9/7. The double-negative feedback loop between CCA1/LHY and PRR5/TOC1 generates a bistable, hysteretic behavior in the Arabidopsis circadian clock. We found that the IFFL involving PRR9/7 breaks the bistability and moves the system forward with a rapid pulse in the daytime, and the evening complex (EC) breaks it in the evening. With this illustration, we can intuitively explain the behavior of the clock under mutant conditions. Thus, our results provide new insights into the underlying network structures of the Arabidopsis core oscillator.},
	language = {eng},
	number = {1},
	journal = {Scientific Reports},
	author = {Joanito, Ignasius and Chu, Jhih-Wei and Wu, Shu-Hsing and Hsu, Chao-Ping},
	year = {2018},
	pmid = {30224713},
	pmcid = {PMC6141573},
	keywords = {Arabidopsis, Arabidopsis Proteins, Circadian Clocks, Circadian Rhythm, Gene Expression Regulation, Plant, Mutation, Photoperiod, Transcription Factors},
	pages = {13944},
}

@article{defoort_function_2018,
	title = {Function, dynamics and evolution of network motif modules in integrated gene regulatory networks of worm and plant},
	volume = {46},
	issn = {0305-1048},
	url = {https://www.ncbi.nlm.nih.gov/pmc/articles/PMC6061849/},
	doi = {10.1093/nar/gky468},
	abstract = {Gene regulatory networks (GRNs) consist of different molecular interactions that closely work together to establish proper gene expression in time and space. Especially in higher eukaryotes, many questions remain on how these interactions collectively coordinate gene regulation. We study high quality GRNs consisting of undirected protein–protein, genetic and homologous interactions, and directed protein–DNA, regulatory and miRNA–mRNA interactions in the worm Caenorhabditis elegans and the plant Arabidopsis thaliana. Our data-integration framework integrates interactions in composite network motifs, clusters these in biologically relevant, higher-order topological network motif modules, overlays these with gene expression profiles and discovers novel connections between modules and regulators. Similar modules exist in the integrated GRNs of worm and plant. We show how experimental or computational methodologies underlying a certain data type impact network topology. Through phylogenetic decomposition, we found that proteins of worm and plant tend to functionally interact with proteins of a similar age, while at the regulatory level TFs favor same age, but also older target genes. Despite some influence of the duplication mode difference, we also observe at the motif and module level for both species a preference for age homogeneity for undirected and age heterogeneity for directed interactions. This leads to a model where novel genes are added together to the GRNs in a specific biological functional context, regulated by one or more TFs that also target older genes in the GRNs. Overall, we detected topological, functional and evolutionary properties of GRNs that are potentially universal in all species.},
	number = {13},
	urldate = {2020-05-06},
	journal = {Nucleic Acids Research},
	author = {Defoort, Jonas and Van de Peer, Yves and Vermeirssen, Vanessa},
	month = jul,
	year = {2018},
	pmid = {29873777},
	pmcid = {PMC6061849},
	pages = {6480--6503},
}

@article{lee_transcriptional_2002,
	title = {Transcriptional regulatory networks in {Saccharomyces} cerevisiae},
	volume = {298},
	issn = {1095-9203},
	doi = {10.1126/science.1075090},
	abstract = {We have determined how most of the transcriptional regulators encoded in the eukaryote Saccharomyces cerevisiae associate with genes across the genome in living cells. Just as maps of metabolic networks describe the potential pathways that may be used by a cell to accomplish metabolic processes, this network of regulator-gene interactions describes potential pathways yeast cells can use to regulate global gene expression programs. We use this information to identify network motifs, the simplest units of network architecture, and demonstrate that an automated process can use motifs to assemble a transcriptional regulatory network structure. Our results reveal that eukaryotic cellular functions are highly connected through networks of transcriptional regulators that regulate other transcriptional regulators.},
	language = {eng},
	number = {5594},
	journal = {Science (New York, N.Y.)},
	author = {Lee, Tong Ihn and Rinaldi, Nicola J. and Robert, François and Odom, Duncan T. and Bar-Joseph, Ziv and Gerber, Georg K. and Hannett, Nancy M. and Harbison, Christopher T. and Thompson, Craig M. and Simon, Itamar and Zeitlinger, Julia and Jennings, Ezra G. and Murray, Heather L. and Gordon, D. Benjamin and Ren, Bing and Wyrick, John J. and Tagne, Jean-Bosco and Volkert, Thomas L. and Fraenkel, Ernest and Gifford, David K. and Young, Richard A.},
