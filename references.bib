	keywords = {Animals, Base Sequence, Binding Sites, Cell Adhesion Molecules, Computer Simulation, DNA-Binding Proteins, Embryo, Nonmammalian, Endoderm, Gastrula, Gene Expression Regulation, Developmental, Lithium Chloride, Models, Genetic, Molecular Sequence Data, Mutagenesis, Promoter Regions, Genetic, Proteins, Sea Urchins, Transcription, Genetic},
	pages = {1896--1902},
