	keywords = {Animals, Bone Marrow Cells, Cell Cycle, Cell Proliferation, Fetus, Hematopoiesis, Hematopoietic Stem Cells, Homeostasis, Liver, Mice, Mice, Knockout, Myeloid-Lymphoid Leukemia Protein},
	pages = {338--345},
