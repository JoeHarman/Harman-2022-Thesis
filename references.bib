	pmid = {12399584},
	keywords = {Algorithms, Cell Cycle, Computational Biology, DNA, Fungal, Feedback, Physiological, Gene Expression Profiling, Gene Expression Regulation, Fungal, Genes, Fungal, Genome, Fungal, Models, Genetic, Promoter Regions, Genetic, Protein Binding, Saccharomyces cerevisiae, Saccharomyces cerevisiae Proteins, Transcription Factors, Transcription, Genetic},
